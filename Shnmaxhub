-- Fun√ß√£o para ativar ou desativar a prote√ß√£o contra o vazio
local function ToggleVoidProtection(bool)
    if bool then
        game.Workspace.FallenPartsDestroyHeight = 0/0
    else
        game.Workspace.FallenPartsDestroyHeight = -9999999999999999
    end
end 

-- Adicionar o bot√£o toggle "Void Protection V4" √† se√ß√£o "Main"
AddToggle(Main, {
    Name = "Void Protection V4",
    Description = "Activate Void Protection V4",
    Default = false,
    Callback = ToggleVoidProtection
})

